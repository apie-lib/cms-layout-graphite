{
  "name": "apie/cms-layout-graphite",
  "description": "Composer package of the apie library: cms layout graphite",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
