<div class="user-profile">
  <a href="{{ property('profileUrl') }}" title="{{ property('email') }}">
    <img src="{{ property('gravatarUrl') }}?s=80" align="right" border="0"/>
  </a>
  <span class="user-info">
    {% for fieldName in property('fieldNames') %}
      <strong>{{ fieldName }}</strong>{{ property('fields')[fieldName] }}
    {% endfor %}
  </span>
</div>